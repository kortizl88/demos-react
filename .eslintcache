roes/HeroScreen.js",[],"/Users/kristianortiz/Documents/pruebasGIT/curso react/demos-react/src/componentes/search/SearchScreen.js",["114","115"],"import React, { useMemo } from 'react'\nimport queryString from 'query-string'\nimport { useLocation } from 'react-router-dom';\nimport { useForm } from '../../hooks/useForm';\nimport { HeroCard } from '../ui/heroes/HeroCard';\nimport getHeroesByName from '../../selectors/getHeroesByName';\n\nexport const SearchScreen = ({history}) => {\n\n    const location = useLocation();\n\n    const { q }  =  queryString.parse( location.search );\n\n    console.log(q);\n\n    const [formValues, handleInputChange, reset] = useForm( {\n        textoBusqueda: q\n    })\n\n    const { textoBusqueda } = formValues;\n\n    const heroesFiltered = useMemo(() => getHeroesByName(q), [q])\n\n    const handleSearch = (e) => {\n        e.preventDefault();\n        history.push(`?q=${textoBusqueda}`)\n    }\n\n    return (\n        <div>\n            <h1>Search Screen</h1>\n            <hr/>\n\n            <div className='row'>\n                <div className='col-5'>\n                    <div>Search form</div>\n                    <hr/>\n                    <form onSubmit={handleSearch}>\n                        <input \n                        type='text'\n                        placeholder='busca tu heroe'\n                        className='form-control'\n                        name='textoBusqueda'\n                        autoComplete=\"off\"\n                        value={textoBusqueda}\n                        onChange={handleInputChange} />\n\n                    <button \n                        type=\"submit\"\n                        className='btn m-1 btn-block btn-outline-primary'>\n                        Buscar\n                    </button>\n\n                    </form>\n                </div>\n\n                <div className=\"col-7\">\n                    <h4>Resultados</h4>\n                    <hr/>\n\n                    {\n                        heroesFiltered.map( hero => (\n                            <HeroCard \n                                key={hero.id}\n                                {...hero} />\n                        ))    \n                    }\n\n                </div>\n\n            </div>\n        </div>\n    )\n}\n","/Users/kristianortiz/Documents/pruebasGIT/curso react/demos-react/src/componentes/ui/heroes/HeroesList.js",[],"/Users/kristianortiz/Documents/pruebasGIT/curso react/demos-react/src/componentes/ui/heroes/HeroCard.js",[],"/Users/kristianortiz/Documents/pruebasGIT/curso react/demos-react/src/selectors/getHeroesByName.js",[],"/Users/kristianortiz/Documents/pruebasGIT/curso react/demos-react/src/selectors/getHeroById.js",["116"],"import heroes from \"../data/heroes\";\n\nconst getHeroesById = (id) => {\n    return heroes.find (hero => hero.id == id);\n}\n\nexport default getHeroesById;","/Users/kristianortiz/Documents/pruebasGIT/curso react/demos-react/src/data/heroes.js",[],"/Users/kristianortiz/Documents/pruebasGIT/curso react/demos-react/src/hooks/useForm.js",[],"/Users/kristianortiz/Documents/pruebasGIT/curso react/demos-react/src/selectors/getHeroeByPublisher.js",["117"],"import heroes from \"../data/heroes\";\n\nconst getHeroesByPublisher = (publisher) => {\n\n    const validPublisher  = ['DC Comics', \"Marvel Comics\"];\n\n    if( !validPublisher.includes( publisher ) ){\n        throw new Error(\"Publisher no valido\")\n    }\n\n    return heroes.filter (hero => hero.publisher == publisher);\n}\n\nexport default getHeroesByPublisher;","/Users/kristianortiz/Documents/pruebasGIT/curso react/demos-react/src/auth/AuthContext.js",[],"/Users/kristianortiz/Documents/pruebasGIT/curso react/demos-react/src/auth/authReducer.js",[],"/Users/kristianortiz/Documents/pruebasGIT/curso react/demos-react/src/types/types.js",[],"/Users/kristianortiz/Documents/pruebasGIT/curso react/demos-react/src/routers/PrivateRoute.js",[],"/Users/kristianortiz/Documents/pruebasGIT